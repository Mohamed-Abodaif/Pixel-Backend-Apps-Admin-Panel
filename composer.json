{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^8.0|^8.1",
        "doctrine/dbal": "^3.5",
        "fruitcake/laravel-cors": "^3.0",
        "hwi/oauth-bundle": "^1.4",
        "laracasts/flash": "^3.2",
        "laravel/framework": "^8.75",
        "laravel/passport": "^10.1",
        "laravel/sanctum": "^2.11",
        "laravel/socialite": "^5.5",
        "laravel/tinker": "^2.5",
        "laravel/ui": "^3.0",
        "laravelcollective/html": "^6.2",
        "lcobucci/clock": "2.2.0",
        "mpdf/mpdf": "^8.0",
        "owen-it/laravel-auditing": "^13.5",
        "owen-oj/laravel-getid3": "1.3",
        "php-http/guzzle6-adapter": "^2.0",
        "php-http/httplug-bundle": "^1.27",
        "prettus/l5-repository": "^2.8",
        "prologue/alerts": "^0.4",
        "rap2hpoutre/fast-excel": "^4.1",
        "sendgrid/sendgrid": "^8.0",
        "spatie/laravel-permission": "^5.5",
        "spatie/laravel-query-builder": "^4.0",
        "spatie/laravel-translatable": "^4.3",
        "stancl/tenancy": "^3.6",
        "symfony/symfony": "5.4.x",
        "tymon/jwt-auth": "^1.0@dev",
        "ext-zip": "*"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "3.5.1",
        "facade/ignition": "^2.5",
        "fakerphp/faker": "^1.9.1",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^5.10",
        "phpunit/phpunit": "^9.5.10"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/",
            "App\\CustomLibs\\" : "app/CustomLibs/",
            "App\\Interfaces\\" : "app/Interfaces/",
            "App\\Helpers\\" : "app/Helpers/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "php-http/discovery": false
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
